{
    "env": {
        "es6": true,
        "node": true
    },
    "extends": [
        "airbnb-base",
        "plugin:@typescript-eslint/eslint-recommended",
        "prettier",
        "plugin:import/typescript"
    ],
    "plugins": ["@typescript-eslint", "prettier"],
    "rules": {
        "class-methods-use-this": 0,
        "import/extensions": [
            "error",
            "ignorePackages",
            {
                "js": "never",
                "jsx": "never",
                "ts": "never",
                "tsx": "never"
            }
        ]
    },
    "parser": "@typescript-eslint/parser",
    "parserOptions": {
        "ecmaVersion": 2018
    }
    // "settings": {
    //     "import/resolver": {
    //         "node": {
    //             "extensions": [".js", ".jsx", ".ts", "tsx"]
    //         }
    //     }
    // }
}
